    "build:tsc": "tsc -b tsconfig.json"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.4.4",
    "@lit/reactive-element": "^1.6.2",
    "@prosekit/core": "^0.0.1",
    "@prosekit/extensions": "^0.0.2",
    "@prosekit/pm": "^0.0.2",
    "lit": "^2.7.6",
    "lit-element": "^3.3.2",
    "lit-html": "^2.7.5"
  },
  "devDependencies": {
    "@prosekit/dev": "*",
    "minify-literals": "^1.0.6",
    "tsup": "^7.1.0",
