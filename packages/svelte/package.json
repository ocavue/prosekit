{
  "name": "@prosekit/svelte",
  "type": "module",
  "version": "0.2.7",
  "private": false,
  "author": {
    "name": "ocavue",
    "email": "ocavue@gmail.com"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/ocavue",
  "homepage": "https://github.com/ocavue/prosekit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ocavue/prosekit.git",
    "directory": "packages/svelte"
  },
  "bugs": {
    "url": "https://github.com/ocavue/prosekit/issues"
  },
  "keywords": [
    "ProseMirror"
  ],
  "sideEffects": false,
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./autocomplete-empty": {
      "svelte": "./src/components/autocomplete-empty.ts",
      "default": "./src/components/autocomplete-empty.ts"
    },
    "./autocomplete-item": {
      "svelte": "./src/components/autocomplete-item.ts",
      "default": "./src/components/autocomplete-item.ts"
    },
    "./autocomplete-list": {
      "svelte": "./src/components/autocomplete-list.ts",
      "default": "./src/components/autocomplete-list.ts"
    },
    "./autocomplete-popover": {
      "svelte": "./src/components/autocomplete-popover.ts",
      "default": "./src/components/autocomplete-popover.ts"
    },
    "./resizable": {
      "svelte": "./src/components/resizable/index.ts",
      "default": "./src/components/resizable/index.ts"
    },
    "./tooltip": {
      "svelte": "./src/components/tooltip/index.ts",
      "default": "./src/components/tooltip/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "svelte-package -i src -o dist/build && tsx scripts/re-export.ts"
  },
  "dependencies": {
    "@prosekit/primitives": "^0.0.0",
    "@prosekit/core": "^0.4.0",
    "@prosekit/lit": "^0.2.8",
    "@prosekit/pm": "^0.1.2"
  },
  "peerDependencies": {
    "svelte": ">= 3.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    }
  },
  "devDependencies": {
    "@prosekit/dev": "*",
    "@sveltejs/package": "^2.3.1",
    "@types/node": "^20.12.7",
    "read-pkg": "^9.0.1",
    "svelte": "^4.2.14",
    "svelte-check": "^3.6.9",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/prosekit-svelte.d.ts",
        "import": "./dist/prosekit-svelte.js",
        "default": "./dist/prosekit-svelte.js"
      },
      "./autocomplete-empty": {
        "types": "./dist/prosekit-svelte-autocomplete-empty.d.ts",
        "svelte": "./dist/prosekit-svelte-autocomplete-empty.js",
        "import": "./dist/prosekit-svelte-autocomplete-empty.js",
        "default": "./dist/prosekit-svelte-autocomplete-empty.js"
      },
      "./autocomplete-item": {
        "types": "./dist/prosekit-svelte-autocomplete-item.d.ts",
        "svelte": "./dist/prosekit-svelte-autocomplete-item.js",
        "import": "./dist/prosekit-svelte-autocomplete-item.js",
        "default": "./dist/prosekit-svelte-autocomplete-item.js"
      },
      "./autocomplete-list": {
        "types": "./dist/prosekit-svelte-autocomplete-list.d.ts",
        "svelte": "./dist/prosekit-svelte-autocomplete-list.js",
        "import": "./dist/prosekit-svelte-autocomplete-list.js",
        "default": "./dist/prosekit-svelte-autocomplete-list.js"
      },
      "./autocomplete-popover": {
        "types": "./dist/prosekit-svelte-autocomplete-popover.d.ts",
        "svelte": "./dist/prosekit-svelte-autocomplete-popover.js",
        "import": "./dist/prosekit-svelte-autocomplete-popover.js",
        "default": "./dist/prosekit-svelte-autocomplete-popover.js"
      },
      "./resizable": {
        "types": "./dist/prosekit-svelte-resizable.d.ts",
        "svelte": "./dist/prosekit-svelte-resizable.js",
        "import": "./dist/prosekit-svelte-resizable.js",
        "default": "./dist/prosekit-svelte-resizable.js"
      },
      "./tooltip": {
        "types": "./dist/prosekit-svelte-tooltip.d.ts",
        "svelte": "./dist/prosekit-svelte-tooltip.js",
        "import": "./dist/prosekit-svelte-tooltip.js",
        "default": "./dist/prosekit-svelte-tooltip.js"
      }
    },
    "main": "./dist/prosekit-svelte.js",
    "module": "./dist/prosekit-svelte.js",
    "types": "./dist/prosekit-svelte.d.ts",
    "typesVersions": {
      "*": {
        ".": [
          "./dist/prosekit-svelte.d.ts"
        ],
        "autocomplete-empty": [
          "./dist/prosekit-svelte-autocomplete-empty.d.ts"
        ],
        "autocomplete-item": [
          "./dist/prosekit-svelte-autocomplete-item.d.ts"
        ],
        "autocomplete-list": [
          "./dist/prosekit-svelte-autocomplete-list.d.ts"
        ],
        "autocomplete-popover": [
          "./dist/prosekit-svelte-autocomplete-popover.d.ts"
        ],
        "resizable": [
          "./dist/prosekit-svelte-resizable.d.ts"
        ],
        "tooltip": [
          "./dist/prosekit-svelte-tooltip.d.ts"
        ]
      }
    }
  }
}
