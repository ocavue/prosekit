{
  "name": "prosekit-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "build": "turbo build",
    "build:force": "turbo build --force",
    "build:package": "turbo build --filter='./packages/*' --concurrency=1",
    "build:website": "turbo build --filter='./website'",
    "website": "pnpm --filter='./website' run dev",
    "playground": "pnpm --filter='./playground' run dev",
    "gen": "turbo run gen --force && pnpm install --prefer-offline --no-frozen-lockfile",
    "test": "vitest",
    "test:coverage": "vitest run --coverage.enabled",
    "test:playground": "pnpm --filter='./playground' run test",
    "typecheck": "tsc --build tsconfig.json",
    "lint": "eslint .",
    "fix": "manypkg fix && turbo run gen --force && eslint . --fix && prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --plugin prettier-plugin-astro --write .",
    "change": "changeset",
    "ci:version": "changeset version && pnpm install --no-frozen-lockfile",
    "ci:version:next": "changeset version --snapshot next && pnpm install --no-frozen-lockfile",
    "ci:publish": "pnpm run build:package && pnpm publish --access public -r --no-git-checks --tag latest",
    "ci:publish:next": "pnpm run build:package && pnpm publish --access public -r --no-git-checks --tag next"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.12",
    "@changesets/cli": "^2.27.1",
    "@changesets/types": "^6.0.0",
    "@manypkg/cli": "^0.21.1",
    "@ocavue/eslint-config": "^1.3.2",
    "@size-limit/esbuild-why": "^11.0.0",
    "@size-limit/preset-small-lib": "^11.0.0",
    "@vitest/coverage-istanbul": "^1.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "size-limit": "^11.0.0",
    "turbo": "^1.10.16",
    "typescript": "^5.3.2",
    "unocss": "^0.58.0",
    "vitest": "^1.0.1"
  },
  "pnpm": {
    "overrides": {
      "typescript": "~5.2.2"
    },
    "patchedDependencies": {
      "prosemirror-highlightjs@0.9.1": "patches/prosemirror-highlightjs@0.9.1.patch"
    }
  }
}
