{
  "name": "prosekit-monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.7.0",
  "scripts": {
    "build": "turbo build",
    "build:force": "turbo build --force",
    "build:package": "turbo build --filter='./packages/*' --concurrency=1",
    "build:example": "turbo build --filter='./examples/*'",
    "build:website": "turbo build --filter='./website'",
    "gen": "turbo run gen --force && pnpm install --prefer-offline --no-frozen-lockfile",
    "test": "vitest",
    "test:coverage": "vitest run --coverage.enabled --coverage.provider=istanbul",
    "typecheck": "tsc --build tsconfig.json",
    "lint": "eslint .",
    "fix": "manypkg fix && turbo run gen --force && eslint . --fix && prettier --plugin prettier-plugin-svelte --plugin prettier-plugin-tailwindcss --plugin prettier-plugin-astro --write .",
    "change": "changeset",
    "ci:version": "turbo run gen && changeset version && pnpm install --no-frozen-lockfile",
    "ci:version:next": "turbo run gen && changeset version --snapshot next && pnpm install --no-frozen-lockfile",
    "ci:publish": "pnpm run build:package && pnpm publish --access public -r --no-git-checks --tag latest",
    "ci:publish:next": "pnpm run build:package && pnpm publish --access public -r --no-git-checks --tag next"
  },
  "dependencies": {
    "@antfu/ni": "^0.21.6",
    "@changesets/cli": "^2.26.2",
    "@manypkg/cli": "^0.21.0",
    "@ocavue/eslint-config": "^1.2.3",
    "@size-limit/esbuild-why": "^8.2.6",
    "@size-limit/preset-small-lib": "^8.2.6",
    "@vitest/coverage-istanbul": "^0.34.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.48.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.2",
    "prettier-plugin-astro": "^0.12.0",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.3",
    "size-limit": "^8.2.6",
    "turbo": "^1.10.13",
    "typescript": "^5.2.2",
    "unocss": "^0.55.3",
    "vitest": "^0.34.3"
  },
  "pnpm": {
    "overrides": {
      "tsup": "https://github.com/ocavue/tsup/tarball/ocavue-release-2023-09-03-c"
    }
  }
}
